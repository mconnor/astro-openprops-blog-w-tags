---
import type { RmUnitType } from '#UnitTypes.js';

import { StackClass } from './stack-l.ts';
interface Props {
  space?: RmUnitType; //"var(--s000)"
  recursive?: boolean; //false
  splitAfter?: number; // no default. The element after which to split the stack with an auto margin
}
const props = Astro.props;
---

<StackClass {...props} client:load>
  <slot />
</StackClass>
