{
  "name": "astro_blog-w-tags",
  "version": "1.0.1",
  "type": "module",
  "author": "Mike Connor",
  "imports": {
    "#*": "./src/*",
    "#unit-types": "./src/UnitTypes.ts",
    "#astro-wc/*": "./src/custom-layout-components/astro-wc/*",
    "#lit-wc/*": "./src/custom-layout-components/lit-wc/*"
  },
  "keywords": [
    "astro",
    "lit",
    "blog",
    "tags",
    "zod"
  ],
  "scripts": {
    "upgradeAstro": "pnpm dlx @astrojs/upgrade",
    "tboff": "astro preferences disable devToolbar",
    "tbon": "astro preferences enable devToolbar",
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check",
    "sync": "astro sync",
    "host": "astro dev --host",
    "force": " astro build -- --force",
    "build": "astro check astro build --verbose",
    "sentry-build": "astro check && astro build && pnpm sentry:sourcemaps",
    "vdeploy": "vercel deploy --prebuilt",
    "devbuild": "astro check && astro build development",
    "preview": "astro preview",
    "astro": "astro",
    "lint:lit-analyzer": "lit-analyzer .",
    "lint": "pnpm run /^lint:.*/",
    "lint:eslint": "eslint . --cache",
    "lint:json": "eslint . -c jsonLintConfig.mjs --cache",
    "fix:json": "eslint . -c jsonLintConfig.mjs --fix",
    "fix": "eslint . --fix .",
    "inspect": "eslint . --inspect-config",
    "inspect:json": "eslint . -c jsonLintConfig.mjs --inspect-config",
    "format": "prettier --write .",
    "stylelint": "stylelint 'src/**/*.css'",
    "sfix": "stylelint \"src/**/*.{css,astro}\" --fix",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org mike-connor --project astro-openprops-blog-w-tags ./dist && sentry-cli sourcemaps upload --org mike-connor --project astro-openprops-blog-w-tags ./dist",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/lit": "^4.3.0",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/rss": "^4.0.8",
    "@astrojs/vercel": "^7.8.1",
    "@iconify-json/mdi": "^1.2.1",
    "@sentry/astro": "^8.34.0",
    "@sentry/browser": "^8.34.0",
    "@sentry/cli": "^2.37.0",
    "@sentry/vite-plugin": "^2.22.5",
    "astro": "^4.16.2",
    "astro-icon": "^1.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "lit": "^3.2.1",
    "open-props": "^1.7.7",
    "sanitize-html": "^2.13.1",
    "sharp": "^0.33.5",
    "vitest": "^2.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@eslint/json": "^0.5.0",
    "@eslint/markdown": "^6.2.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-markdown": "^2.0.2",
    "@types/eslint__js": "^8.42.3",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "astro-eslint-parser": "^1.0.3",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-wc": "^2.2.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lit-analyzer": "^2.0.3",
    "postcss": "^8.4.47",
    "postcss-custom-media": "^11.0.3",
    "postcss-html": "^1.7.0",
    "postcss-import": "^16.1.0",
    "postcss-jit-props": "^1.0.14",
    "postcss-nesting": "^13.0.0",
    "postcss-pow": "^1.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.10.0",
    "stylelint-config-html": "^1.1.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.8.1",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">= 20.11.0 <21 || >= 21.2.0",
    "pnpm": ">=9.1.2"
  },
  "packageManager": "pnpm@9.12.1",
  "lint-staged": {
    "*.{js,mjs,ts,astro}": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}